<mvc:View
	controllerName="InventCustomControls.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:tnt="sap.tnt"
	height="90%">
	<tnt:ToolHeader>
			
		
		<Text text="{i18n>App.Name}" wrapping="false">
			<layoutData>
				<OverflowToolbarLayoutData priority="Disappear" />
			</layoutData>
		</Text>
		<ToolbarSpacer />
		
			
	</tnt:ToolHeader>
	<App id="app" />
</mvc:View> 